# Setting PATH for Python 3.7
# The original version is saved in .bash_profile.pysave
alias start-tomcat='/opt/tomcat/bin/startup.sh'
alias tomcat-log='tail -f /opt/tomcat/logs/catalina.* /opt/tomcat/logs/localhost.*'
alias rm-log='rm -fr /opt/tomcat/logs/*'
alias tomcat-ps='ps -ef | grep tomcat'

alias kill-nginx='sudo pkill -9 -f nginx'
alias kill-tomcat='pkill -9 -f tomcat'

alias flush-redis='redis-cli FLUSHALL'
alias flush-memcached='echo 'flush_all' | nc localhost 11211'

flush-all(){
        flush-memcached
        flush-redis
}



